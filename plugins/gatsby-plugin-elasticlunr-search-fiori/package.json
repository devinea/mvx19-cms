  "main": "n/a",
  "scripts": {
    "build": "babel src --out-dir .",
    "build:watch": "babel src --watch --out-dir .",
    "default": "npm run build:watch",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "format": "prettier --write '**/*.{js,md,json}'",
    "publish": "npm run lint && npm publish --access=public",
